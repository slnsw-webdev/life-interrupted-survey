                        <input type="button" name="q6btn" class="multiple-button" value="Timeline" />
                        <input type="button" name="q6btn" class="multiple-button" value="iPads" />
                        <input type="button" name="q6btn" class="multiple-button" value="WWI website" />
                        <input type="button" name="q6btn" class="multiple-button" value="Curio app" />
                        <input type="button" name="q6btn" class="multiple-button" value="Events and tours" />
