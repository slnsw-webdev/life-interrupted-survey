                        <input type="button" name="q3btn" class="multiple-button" value="State Library internal signage" />
                        <input type="button" name="q3btn" class="multiple-button" value="State Library social media" />
                        <input type="button" name="q3btn" class="multiple-button" value="State Library website" />
                        <input type="button" name="q3btn" class="multiple-button" value="Daily Telegraph print advertisement" />
                        <input type="button" name="q3btn" class="multiple-button" value="Newspaper article" />
                        <input type="button" name="q3btn" class="multiple-button" value="Inside History magazine" />
                        <input type="button" name="q3btn" class="multiple-button" value="Time Out Sydney" />
                        <input type="button" name="q3btn" class="multiple-button" value="What’s On in Sydney" />
                        <input type="button" name="q3btn" class="multiple-button" value="Radio" />
                        <input type="button" name="q3btn" class="multiple-button" value="Word-of-mouth" />
