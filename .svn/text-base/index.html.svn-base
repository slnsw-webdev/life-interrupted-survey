                        <input type="button" name="next" class="next action-button" value="Next" />
                    </fieldset>


                    <fieldset>
                        <h2 class="fs-title">Do you have your own device with you today?</h2>
                        <input type="button" name="q15btn" class="multiple-button" value="Mobile" />
