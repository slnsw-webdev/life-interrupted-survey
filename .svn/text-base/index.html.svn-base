        <!-- survey logo -->
		<div id="container">
        
            <div id="header">
                <div id="logocontainer">
                    <div id="title">Survey Form</div>
                </div>
                <div id="searchform"></div>
                <div id="sllogocontainer">
                    <img src="img/sl_logo.png" id="sl_logo" name="sl_logo">
                </div>
            </div>
            
            <div id="main" role="main">
            	
                <div id="surveyheader">
                	<img src="img/survey_logo.png" />
                    <hr />
                </div>
    			
                <div id="barcontainer">
    				<!-- progressbar -->
                	<div id="progressbar" style="height: 20px;"></div>
    			</div>
                
                <!-- multistep form -->
                <form id="msform">
                    
                    <!-- fieldsets -->
                    <fieldset>
                        <h1>Please complete our survey and enter the draw to win a set of photographic prints</h1>
                        <p>Your feedback will help us improve our exhibitions. The survey will take approximately five minutes.</p>
                        <p>&nbsp;</p>


                        <h2 class="fs-title">Have you visited Life Interrupted before?</h2>
                        <input type="button" name="q1btn" class="single-button" value="Yes" />
                        <input type="button" name="q1btn" class="single-button" value="No" />
                        <input type="button" name="next" class="next action-button" value="Next" />
                    </fieldset>
                    <fieldset>
                        <h2 class="fs-title">Is Life Interrupted the main reason you are visiting the State Library today?</h2>
                        <input type="button" name="q2btn" class="single-button" value="Yes" />
                        <input type="button" name="q2btn" class="single-button" value="No" />
                        <input type="button" name="previous" class="previous action-button" value="Previous" />
                        <input type="button" name="next" class="next action-button" value="Next" />
                    </fieldset>
                    <fieldset>
                        <h2 class="fs-title">How did you hear about Life Interrupted?</h2>
                        <input type="button" name="q3btn" class="multiple-button" value="State Library Curio app" />
